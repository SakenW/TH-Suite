{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:default",
    "fs:read-all",
    "fs:write-all",
    "fs:scope",
    "notification:default",
    "shell:default",
    "http:default",
    "process:default",
    "window-state:default"
  ]
}